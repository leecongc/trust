<scroll-view scroll-y="true">
  <view class='body'>
      <view class='title'>
          <image src='../../images/logo.png' mode="aspectFit" />
          <!-- <text>信儿工作室</text> -->
          <text>{{user}}</text>
      </view>
       <view class='content'> 
          <text class='art_title'>{{title}}</text>
          <template is="wxParse" data="{{wxParseData:content.nodes}}"/>
           <!-- {{content}}  -->
           <!-- {{datass}}  -->
       </view> 
      <view class='foot'>
        <text>{{date}}</text>
      </view>
  </view>
</scroll-view>
